{
  "name": "cngraps",
  "version": "1.0.0",
  "description": "Complete Nodejs Graphql Reactjs Apollo Postgres Sandbox",
  "main": "dist/index.js",
  "repository": "git@github.com:Thithi32/cngraps.git",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "sh -ac ' . ./.env; node_modules/sequelize-cli/lib/sequelize db:migrate'",
    "migrate:undo": "sh -ac ' . ./.env; node_modules/sequelize-cli/lib/sequelize db:migrate:undo'",
    "server-dev": "nodemon --exec \"node --require 'dotenv/config' --require 'babel-register' src/index.js\"",
    "client-dev": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server-dev\" \"npm run client-dev\"",
    "prod": "sh -ac ' . ./.env; node dist/index.js'",
    "server-build": "babel src -d dist --copy-files",
    "client-build": "cd client && npm run build && mkdir -p ../dist/client && cp -r build ../dist/client/",
    "client-install": "cd client && npm install",
    "build": "concurrently --kill-others-on-fail \"npm run server-build\" \"npm run client-build\"",
    "postinstall": "node_modules/sequelize-cli/lib/sequelize db:migrate && npm run client-install && npm run build",
    "start": "node dist/index.js",
  },
  "cacheDirectories": ["client/node_modules", "node_modules"],
  "keywords": [
    "graphql",
    "express",
    "postgres"
  ],
  "author": "Thierry D.",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "graphql": "^0.12.3",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^2.19.0",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "mustache": "^2.3.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "regenerator-runtime": "^0.11.1",
    "sendgrid": "^5.2.3",
    "sequelize": "^4.32.1",
    "sequelize-cli": "^3.2.0",
    "sequelize-replace-enum-postgres": "^1.3.0",
    "slack-node": "^0.1.8",
    "subscriptions-transport-ws": "^0.9.7",
    "tableify": "^1.1.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nodemon": "^1.14.11",
    "concurrently": "^3.5.0"
  }
}
